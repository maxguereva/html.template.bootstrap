{
	"name" : "mouf/html.template.bootstrap",
	"description" : "This package contains a Mouf template based on the Twitter Bootstrap HTML/CSS/JS library, along a Mouf installer file to add the template easily in your Mouf project.",
	"type" : "mouf-library",
	"authors" : [{
			"name" : "David NÃ©grier",
			"email" : "d.negrier@thecodingmachine.com",
			"homepage" : "http://mouf-php.com"
		}
	],
	"keywords" : [
		"mouf",
		"bootstrap",
		"template"
	],
	"homepage" : "http://twitter.github.com/bootstrap/",
	"license" : "MIT",
	"require" : {
		"mouf/html.utils.weblibrarymanager" : "~3.0",
	        "mouf/html.utils.weblibrarymanager.component-installer" : "~2.2",
		"mouf/html.widgets.messageservice" : "~3.0",
		"php" : ">=5.4.0",
		"mouf/html.widgets.menu" : "~3.0",
		"mouf/html.template.templateinterface" : "~2.1"
	},
	"autoload" : {
		"psr-0" : {
			"Mouf" : "src/"
		}
